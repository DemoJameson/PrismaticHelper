<?xml version="1.0" encoding="utf-8" ?>
<Sprites>
    <PrismaticHelper_crushblock_face path="PrismaticHelper/cassetteKevin/" start="idle">
        <Center/>
        <Loop id="idle" path="idle_face" delay="0.08"/>
        <Anim id="hurt" path="hurt" frames="3-12" delay="0.08" goto="idle"/>
        <Anim id="hit" path="hit" delay="0.08"/>
        <Loop id="left" path="hit_left" delay="0.08"/>
        <Loop id="right" path="hit_right" delay="0.08"/>
        <Loop id="up" path="hit_up" delay="0.08"/>
        <Loop id="down" path="hit_down" delay="0.08"/>
    </PrismaticHelper_crushblock_face>

    <PrismaticHelper_giant_crushblock_face path="PrismaticHelper/cassetteKevin/" start="idle">
        <Center/>
        <Loop id="idle" path="giant_block" frames="0" delay="0.08"/>
        <Anim id="hurt" path="giant_block" frames="8-12" delay="0.08" goto="idle"/>
        <Anim id="hit" path="giant_block" frames="0-5" delay="0.08"/>
        <Loop id="right" path="giant_block" frames="6,7" delay="0.08"/>
    </PrismaticHelper_giant_crushblock_face>

    <PrismaticHelper_ice path="PrismaticHelper/temperatureBlocks/ice/iceblock" start="idle">
        <Loop id="idle" path="" frames="1" delay="0.08"/>
        <Anim id="disappear" path="" frames="2-5" delay="0.08" goto="blank"/>
        <Loop id="blank" path="" frames="7" delay="0.08"/>
        <Anim id="appear" path="" frames="8-12" delay="0.08" goto="idle"/>
    </PrismaticHelper_ice>

    <PrismaticHelper_steam path="PrismaticHelper/temperatureBlocks/steam/steamblock" start="idle">
        <Loop id="idle" path="" frames="1" delay="0.08"/>
        <Anim id="disappear" path="" frames="2-5" delay="0.08" goto="blank"/>
        <Loop id="blank" path="" frames="7" delay="0.08"/>
        <Anim id="appear" path="" frames="8-12" delay="0.08" goto="idle"/>
    </PrismaticHelper_steam>

    <PrismaticHelper_speed_berry path="PrismaticHelper/collectables/speedBerry/" start="idle">
        <Center/>

        <Loop id="idle" path="Idle_g" frames="0-9" delay="0.08"/>
        <Anim id="collect" path="collect_g" frames="0-3" delay="0.08"/>
        <Anim id="explode" path="explode_" frames="0-7" delay="0.08"/>
        <Anim id="transition" path="transition_b" frames="0-11" delay="0.1"/>
    </PrismaticHelper_speed_berry>

    <PrismaticHelper_cas_berry_blue path="PrismaticHelper/collectables/cassette_berries/cas_berry_blue/" start="idle">
        <Center/>

        <Loop id="idle" path="normal" delay=".1" frames="0-6,0-6,0-6,0-6,0-7"/>
        <Loop id="deactive" path="deactive/normal" delay=".1" frames="0-6"/>
        <Loop id="noFlash" path="normal" delay=".1" frames="0-6"/>
        <Anim id="collect" path="normal" delay=".07" frames="7-11"/>
        <Loop id="flap" path="wings" delay=".08" frames="0-8,0-8,0-7,9"/>
        <Loop id="flap_deactive" path="deactive/wings" delay=".08" frames="0-8,0-8,0-7,9"/>

        <Anim id="fade0" path="normal" delay=".08" frames="12-14,14*10,15-19"/>
        <Anim id="fade1" path="normal" delay=".07" frames="20-22,22*10,23-26"/>
        <Anim id="fade2" path="normal" delay=".07" frames="28-30,30*10,31-34"/>
        <Anim id="fade3" path="normal" delay=".07" frames="36-38,38*10,39-42"/>
        <Anim id="fade4" path="normal" delay=".07" frames="44-46,46*10,47-50"/>
        <Anim id="fade5" path="normal" delay=".07" frames="52,53,54,53,54,53,54,53,54,53,54,53,54-59"/>
        <Anim id="fade_wow" path="wow/normal" delay=".1" frames="52-66"/>
    </PrismaticHelper_cas_berry_blue>
    
    <PrismaticHelper_cas_berry_rose path="PrismaticHelper/collectables/cassette_berries/cas_berry_rose/" copy="PrismaticHelper_cas_berry_blue"/>
    <PrismaticHelper_cas_berry_sun path="PrismaticHelper/collectables/cassette_berries/cas_berry_sun/" copy="PrismaticHelper_cas_berry_blue"/>
    <PrismaticHelper_cas_berry_malachite path="PrismaticHelper/collectables/cassette_berries/cas_berry_malachite/" copy="PrismaticHelper_cas_berry_blue"/>
    
    <PrismaticHelper_cas_ghostberry_blue path="PrismaticHelper/collectables/cassette_ghostberries/cas_berry_blue/" copy="PrismaticHelper_cas_berry_blue"/>
    <PrismaticHelper_cas_ghostberry_rose path="PrismaticHelper/collectables/cassette_ghostberries/cas_berry_rose/" copy="PrismaticHelper_cas_berry_blue"/>
    <PrismaticHelper_cas_ghostberry_sun path="PrismaticHelper/collectables/cassette_ghostberries/cas_berry_sun/" copy="PrismaticHelper_cas_berry_blue"/>
    <PrismaticHelper_cas_ghostberry_malachite path="PrismaticHelper/collectables/cassette_ghostberries/cas_berry_malachite/" copy="PrismaticHelper_cas_berry_blue"/>
    
    <PrismaticHelper_cas_headphones path="PrismaticHelper/collectables/cassette_berries/cas_headphones/" start="idle">
        <Center/>

        <Loop id="idle" path="headphones" delay=".08" frames="0-8,0-8,0-7,9"/>
    </PrismaticHelper_cas_headphones>
    
    <PrismaticHelper_amalgamated_cassette_block path="PrismaticHelper/amalgam/" start="idle">
        <Loop id="idle" path="solid"/>
        <Loop id="pressed" path="pressed"/>
    </PrismaticHelper_amalgamated_cassette_block>
</Sprites>